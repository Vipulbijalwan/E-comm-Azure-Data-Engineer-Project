{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ecomm-factory"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"FileSqlHttp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileSqlHttp'"
		},
		"SQL_TO_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SQL_TO_ADLS'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ecommstores.dfs.core.windows.net/"
		},
		"FileSqlHttp_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "fq61ix.h.filess.io"
		},
		"FileSqlHttp_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistproject_articlebow"
		},
		"FileSqlHttp_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "olistproject_articlebow"
		},
		"HttpGithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"SQL_TO_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ecommstores.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ecomm-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"FileSqlHttp_password": {
						"value": "[parameters('FileSqlHttp_password')]"
					},
					"SQL_TO_ADLS_accountKey": {
						"value": "[parameters('SQL_TO_ADLS_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"FileSqlHttp_properties_typeProperties_server": {
						"value": "[parameters('FileSqlHttp_properties_typeProperties_server')]"
					},
					"FileSqlHttp_properties_typeProperties_database": {
						"value": "[parameters('FileSqlHttp_properties_typeProperties_database')]"
					},
					"FileSqlHttp_properties_typeProperties_username": {
						"value": "[parameters('FileSqlHttp_properties_typeProperties_username')]"
					},
					"HttpGithub_properties_typeProperties_url": {
						"value": "[parameters('HttpGithub_properties_typeProperties_url')]"
					},
					"SQL_TO_ADLS_properties_typeProperties_url": {
						"value": "[parameters('SQL_TO_ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}